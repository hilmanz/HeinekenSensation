#navigation select {
	display: none;
	width: 70%;
	padding: 5px 15px;
	background: #f5f5f5;
	color: #777;
	border: none;
	font-size: 14px;
	margin: 20px auto;
	-webkit-border-radius: 30px;
	-moz-border-radius: 30px;
	-o-border-radius: 30px;
	border-radius: 30px;
}
.width-700,.w-mob{
	display:none;
}
/* Responsive */
@media only screen and (max-width: 1100px) {
	.wrapper {
		width: 95%;
	}
	.twitshare{
		top:35px;
		right:0;
	}
	.fbshare{
		top:35px;
		right:74px;
	}
	.inputBirth input[type=text], .inputBirth input[type=number]{
		font-size:40px;
	}
}
@media only screen and (max-width: 900px) {
	.columns-content li.col-main,.columns-content li.col-sidebar{
		clear:both;
		float:none;
		width:100%;
		margin:0 0 20px 0;
	}
	#bodyLanding .col2{
		width:100%;
	}
	#videoLanding{
		top:auto;
		padding:30px 0;
	}
	#videoLanding{
		left:auto;
		z-index:2;
	}
}

@media only screen and (max-width: 800px) {
	.columns-content li.col-main,
	.columns-content li.col-sidebar {
		float: none;
		width: 100%;
		margin: 0;
	}
	.widget{
		max-width:inherit;
	}
	.width-700{
		display:block;
	}
	.width-1000{
		display:none;
	}
	.popup,.popupBig{
		background:#004021;
		width:580px;
		overflow:hidden !important;
		height:auto;
	}
	.popupEntry,.popupBig .popupEntry{
		padding:20px 0;
		text-align:center;
	}
	.popupBig .popupEntry{
		text-align:left;
	}
	.popupEntry .greenBtn{
		margin:10px auto;
	}
	.popupEntry .col3{
		width:100%;
		float:none;
	}
	.quizForm h3{
		font-size:14px;
	}
	#notFan{
		position:relative;
		left:auto;
		top:auto;
		margin:100px auto;
	}
	#universal{
		background:none;
	}
	
}

@media only screen and (max-width: 667px) {
	
	.helpIcon{
		display:none;
	}
	.helpIcon2{
		display:block;
	}
	.boxRight{
		display:none;
	}
	.boxLeft{
		width:70%;
	}
	.boxLeft .helpBox4,.boxLeft .helpBox5,.boxLeft .helpBox6{
		display:block;
	}
	.columns-4 li.col4,.columns-2 li.col2,.columns-3 li.col3{
		width:100%;
		float:none;
		clear:both;
		margin:0;
	}
	.boxLeft .helpBox{
		margin:0 0 52px 20px;
	}
	.boxLeft .helpBox5{
		margin:0 0 68px 20px;
	}
	.boxLeft .helpBox3{
		margin:0 0 68px 20px;
	}
	#header{
		height:auto;
		overflow:hidden;
	}
	.logoSensation{
		float:none;
		top:auto;
		margin:20px auto 40px auto;
	}
	#navigation{
		top:auto;
	}
	#main-menu{
		float:none;
		text-align:center;
	}
	#main-menu a{
		padding:0 10px;
	}
	#main-menu li{
		float:none;
		display:inline-block;
	}
	.imgBox{
		width:50%;
	}
	.popupContainer{
		display:table;
		padding:30px;
	}
	.quizForms .row{
		padding:10px 0;
	}
	#userBox #PleaseCompleteQuiz{
		clear:both;
		position:relative;
		top:auto;
		left:auto;
		right:auto;
		float:none;
		margin:10px 10px 0 0;
		display:block;
	}
	.ekenLogo{
		display:none;
	}
	#notFan{
		width:215px;
	}
}

@media only screen and (max-width: 479px) {
	.iconHelp{
		display:block;
		text-align:center;
	}
	#helpContainer,.boxLeft{
		width:auto;
		text-align:center;
	}
	.boxLeft .helpBox{
		margin:0 0 20px 0;
	}
	.helpIcon2{
		display:none;
	}
	.widget,.the-contents ,#userBox{
		max-width:285px;
		margin:0 auto 20px auto;
	}
	
	.columns-2 li.col2 {
		width: 100%;
		margin: 0;
	}
	.columns-3 li.col3 {
		float: none;
		width: 100%;
		margin: 0;
	}
	#userBox .greenBtn{
		top:auto;
		float:right;
		margin:7px 0 0 0;
		clear:both;
		display:block;
		width:80px;
		font-size:7pt;
		text-align:center;
	}
	.popup,.popupBig{
		background:#004021;
		height:auto;
		width:240px;
		overflow:hidden !important;
	}
	#videoLanding,#video-landing{
		width:290px !important;
		height:218px !important;
	}
	#body{
		max-width:320px;
		margin:0 auto;
	}
	.popupEntry .greenBtn{
		width:100px;
		font-size:14px;
	}
	#main-menu a{
		font-size:8pt;
		padding:0 5px;
	}
	.smallThumb{
		width:60px; height:60px;
	}
	.smallThumb img{
		width:60px;
	}
	#theContent {
		width: 80%;
	}
	.quizForms .col4{
		width:100%;
		float:none;
		overflow:hidden;
	}
	.quizForms label{
		width:80%;
		float:left;
		text-align:left;
	}
	.quizForm h3, .quizForms h3{
		font-size:14px;
	}
	.quizForm .select{
		font-size:7pt;
   	    font-family: 'RobotoCondensed';
	}
	#landingEntry{
		max-width:300px;
		margin:0 auto;
	}
	.inputBirth input[type=text], .inputBirth input[type=number]{
		width:60px; height:60px;
		font-size:20px;
	}
	#landingEntry h1{
		font-size:24px;
	}
	.user-info{
		width:185px;
	}
	#userBox .smallThumb{
		margin:5px 10px 0 0;
	}
	#userBox .smallThumb img{
		width:100%;
		height:auto;
	}
	.widgetGallery a,.widgetGallery a img{
		width:85px; height:85px;
	}
	.w-mob{
		display:block;
		padding:10px 0;
	}
	.w-web{
		display:none;
	}
	.twitshare,.fbshare{
		position:relative;
		top:auto;
		float:left;
		margin:0 10px 0 0;
		right:auto;
		display:block;
		padding:10px 0;
		overflow:hidden;
	}
	#helpContainer .page-title .fr,
	#helpContainer .page-title .fl{
		float:none;
	}
	#finishTrivia{
		width:185px;
	}
}

@media only screen and (max-width: 320px) {
	
}